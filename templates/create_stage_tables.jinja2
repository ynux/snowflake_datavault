{%- for row in rows %}
{%- if loop.index == 1 %}
    {{ row['table'] | lower }} = Table('{{ row['table'] }}', metadata, 
{%-endif%}
        Column('{{ row['column'] }}', {{ row['data_type'] | replace("TEXT","String") |  replace("NUMBER","Integer") |  replace("DATE","Date") |  replace("TIMESTAMP_NTZ","TIMESTAMP_NTZ") }}, nullable={{ row['nullable'] | replace("YES","True") |  replace("NO","False") }}){{ "," if not loop.last else "\n    )" }}
{%-endfor%}
